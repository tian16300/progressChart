<div class="circleProgress-container">
    <div class="circleProgress-box">
        <div class="text_wrapper">
            <div class="num">0</div>
            <div class="unit">{{unitText}}</div>
        </div>
        <div class="circleProgress_wrapper min">
            <div class="dotted-circle"></div>
        </div>
        <div class="circleProgress_wrapper mid">
            <div class="solid-circle"></div>
        </div>
        <div class="circleProgress_wrapper max">
            <div class="wrapper right">
                <div class="circleProgress rightcircle"></div>
            </div>
            <div class="wrapper left">
                <div class="circleProgress leftcircle"></div>
            </div>
        </div>
    </div>
    <div class="text-name">
        {{title}}
    </div>
</div>